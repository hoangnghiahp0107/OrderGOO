<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.SettingFragment">

    <!-- TODO: Update blank fragment layout -->
    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FAF6F6">
        <TextView
            android:id="@+id/tvRecommendations1"
            android:layout_width="119dp"
            android:layout_height="34dp"
            android:layout_marginTop="428dp"
            android:text="Recommendations1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.003"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <TextView
            android:id="@+id/tvRecommendations2"
            android:layout_width="119dp"
            android:layout_height="34dp"
            android:layout_marginTop="532dp"
            android:text="Recommendations2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tvEditText"
            android:layout_width="119dp"
            android:layout_height="34dp"
            android:layout_marginTop="152dp"
            android:text="Edit text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.006"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <TextView
            android:id="@+id/tv_notifications"
            style="?android:attr/listSeparatorTextViewStyle"
            android:layout_width="408dp"
            android:layout_height="49dp"
            android:layout_marginTop="364dp"
            android:drawableLeft="@drawable/ic_notification"
            android:text="Notifications"
            android:textColorHighlight="#934444"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <TextView
            android:id="@+id/textView7"
            android:layout_width="103dp"
            android:layout_height="64dp"
            android:text="Settings"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.003"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.023" />

        <TextView
            android:id="@+id/tvUseThePartnerInformation1"
            android:layout_width="229dp"
            android:layout_height="34dp"
            android:layout_marginTop="480dp"
            android:text="Use the partner's information1"
            android:textAlignment="gravity"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tvUseThePartnerInformation2"
            android:layout_width="223dp"
            android:layout_height="33dp"
            android:layout_marginTop="584dp"
            android:text="Use the partner's information2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tvChangePassword"
            android:layout_width="119dp"
            android:layout_height="34dp"
            android:layout_marginTop="192dp"
            android:text="Change password"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tvYourPreferences"
            android:layout_width="119dp"
            android:layout_height="34dp"
            android:layout_marginTop="248dp"
            android:text="Your preferences"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tvInviteFriends"
            android:layout_width="118dp"
            android:layout_height="42dp"
            android:layout_marginTop="296dp"
            android:text="Invite friends"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.003"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tvAccount"
            style="?android:attr/listSeparatorTextViewStyle"
            android:layout_width="408dp"
            android:layout_height="49dp"
            android:layout_marginTop="84dp"
            android:drawableLeft="@drawable/account_icon"
            android:text="Account"
            android:textColorHighlight="#934444"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Switch
            android:id="@+id/switch1"
            android:layout_width="117dp"
            android:layout_height="50dp"
            android:layout_marginTop="468dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.891"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Switch
            android:id="@+id/switch2"
            android:layout_width="119dp"
            android:layout_height="49dp"
            android:layout_marginTop="516dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.892"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Switch
            android:id="@+id/switch3"
            android:layout_width="116dp"
            android:layout_height="48dp"
            android:layout_marginTop="428dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.894"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Switch
            android:id="@+id/switch4"
            android:layout_width="116dp"
            android:layout_height="51dp"
            android:layout_marginTop="564dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.888"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/btn_log_out"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="644dp"
            android:backgroundTint="@android:color/white"
            android:drawableLeft="@drawable/ic_notification_log_out"
            android:text="Log Out"
            android:textColor="@android:color/black"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/btn_exit"
            android:layout_width="63dp"
            android:layout_height="60dp"
            android:layout_marginTop="4dp"
            android:background="@drawable/ic_exit"
            android:backgroundTint="#120202"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.991"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:contentDescription="Exit button"/>

        <ImageButton
            android:id="@+id/imgBtn_EditText"
            android:layout_width="96dp"
            android:layout_height="48dp"
            android:layout_marginTop="152dp"
            android:background="#FAF8F8"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.947"
            app:layout_constraintStart_toStartOf="@+id/tvEditText"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_account_section"
            android:contentDescription="Edit Text Button" />

        <ImageButton
            android:id="@+id/imgbtn_ChangePassword"
            android:layout_width="95dp"
            android:layout_height="48dp"
            android:layout_marginTop="192dp"
            android:background="#FAF9F9"

            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.953"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_account_section"
            android:contentDescription="Change Password Button"/>

        <ImageButton
            android:id="@+id/imgbtn_YourPrefences"
            android:layout_width="94dp"
            android:layout_height="48dp"
            android:layout_marginTop="248dp"
            android:background="#F8F6F6"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.953"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_account_section"
            android:contentDescription="Your Prefences Button"/>

        <ImageButton
            android:id="@+id/imgbtn_InviteFriends"
            android:layout_width="91dp"
            android:layout_height="48dp"
            android:layout_marginTop="296dp"
            android:background="#FBFAFA"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.951"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_account_section"
            android:contentDescription="Invite Friends button"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>